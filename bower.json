{
  "name": "XML-to-Bootstrap",
  "description": "Transforms a generic XML document into Bootstrap HTML pages",
  "authors": ["Achim Christ"],
  "license": "MIT",
  "homepage": "https://github.com/acch/XML-to-bootstrap",
  "devDependencies": {
    "bootstrap": "~4.*",
    "scrollpos-styler": "*",
    "font-awesome": "*",
    "headroom.js": "*",
    "photoswipe": "*"
  },
  "exportsOverride": {
    "bootstrap": {
      "css": "dist/css/*.min.css",
      "js": "dist/js/*.min.js",
      "less": "less/variables.less",
      "sass": ["scss/_variables.scss", "scss/mixins/_breakpoints.scss"]
    },
    "font-awesome": {
      "css": "css/font-awesome.min.css"
    }
  }
}
